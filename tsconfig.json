{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 22",
	"_version": "22.14.0",
	"compilerOptions": {
		"target": "es2022",
		"lib": ["ES2023"],
		"module": "Node16",
		"rootDir": "./",
		"moduleResolution": "node16",
		"baseUrl": "./",
		"paths": {
			"@*": ["./src/*"]
		},
		"types": ["vitest/globals"],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
